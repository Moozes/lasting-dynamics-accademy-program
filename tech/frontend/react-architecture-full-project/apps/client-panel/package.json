        "regenerator-runtime": "^0.13.11",
        "ui": "*",
        "vite": "^4.1.1",
        "vite-plugin-checker": "^0.5.5",
        "vite-plugin-eslint": "^1.8.1",
        "xlsx": "^0.18.5",
        "yup": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@storybook/addon-actions": "^6.5.16",
