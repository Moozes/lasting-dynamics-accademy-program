        "@react-pdf/types": "2.3.3",
        "@react-spring/web": "^9.7.2",
        "@sentry/react": "7.40.0",
        "@sentry/tracing": "7.40.0",
        "@tanstack/react-query": "^4.24.10",
        "@tanstack/react-query-devtools": "^4.26.0",
        "@types/lodash": "^4.14.191",
        "@types/node": "^20.14.2",
        "@vitejs/plugin-react": "^3.1.0",
        "axios": "^1.3.4",
        "clsx": "^1.2.1",
        "date-fns": "^2.29.3",
        "eslint": "^8.34.0",
        "file-saver": "^2.0.5",
        "firebase": "^9.17.1",
        "formik": "^2.2.9",
        "html-react-parser": "^5.1.10",
        "html2canvas": "^1.4.1",
        "i18next": "^22.4.9",
        "i18next-browser-languagedetector": "^7.0.1",
        "jotai": "^2.0.2",
        "lodash": "^4.17.21",
        "notistack": "^3.0.1",
        "random-words": "^1.3.0",
        "react": "^18.2.0",
