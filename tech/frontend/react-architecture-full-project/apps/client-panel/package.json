{
    "name": "client-panel",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:staging": "vite --mode staging",
        "build:production": "tsc && vite build --mode production && yarn postbuild",
        "build:development": "tsc && vite build --mode development && yarn postbuild",
        "build:staging": "tsc && vite build --mode staging",
        "preview": "vite preview",
        "lint": "eslint .",
        "test": "vitest run",
        "lint-fix": "eslint . --fix",
        "prettier-check": "prettier --check src/**/* --print-width 120 --tab-width 4",
        "format": "prettier --write src/**/* --print-width 120 --tab-width 4",
        "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6106",
        "build-storybook": "build-storybook",
        "postbuild": "mkdir -p build/.well-known && cp -r public/.well-known build/"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
